{
  "name": "opencerts-admin-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next build && next start",
    "cname": "echo 'admin.opencerts.io' > out/CNAME",
    "deploy": "rm -rf out && npm run build:static",
    "deploy:legacy": "npm run deploy:prebuilt && gh-pages --dotfiles --dist out",
    "deploy:prebuilt": "rm -rf out && npm run build:static && touch out/.nojekyll && npm run cname"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@ledgerhq/hw-transport-u2f": "4.48.0",
    "@ledgerhq/web3-subprovider": "4.48.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "eslint-plugin-react": "^7.7.0",
    "lodash": "^4.17.5",
    "next": "^8.0.0",
    "next-ga": "^2.3.4",
    "next-redux-wrapper": "^2.0.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.1",
    "react-tabs": "^2.2.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "truffle-contract": "^3.0.4",
    "web3": "1.0.0-beta.36",
    "web3-provider-engine": "14.0.6",
    "web3-utils": "^1.0.0-beta.36"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gh-pages": "^1.1.0",
    "jest": "^24.5.0",
    "prettier": "^1.10.2",
    "webpack": "^4.29.6"
  }
}
